{
  "name": "default",
  "type": "sqlite",
  "database": "database.sqlite",
  "synchronize": true,
  "logging": false,
  "entities": [
    "${rootDir}/domain/**/*.{js,ts}"
  ],
  "migrations": [
    "${rootDir}/persistence/migration/**/*.{js,ts}"
  ],
  "subscribers": [
    "${rootDir}/persistence/subscriber/**/*.{js,ts}"
  ],
  "cli": {
    "entitiesDir": "${rootDir}/domain",
    "migrationsDir": "${rootDir}/persistence/migration",
    "subscribersDir": "${rootDir}/persistence/subscriber"
  }
}
