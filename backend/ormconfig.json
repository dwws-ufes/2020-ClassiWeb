{
   "type": "sqlite",
   "database": "database.sqlite",
   "synchronize": true,
   "logging": false,
   "entities": [
      "src/domain/**/*.ts"
   ],
   "migrations": [
      "src/persistence/migration/**/*.ts"
   ],
   "subscribers": [
      "src/persistence/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/domain",
      "migrationsDir": "src/persistence/migration",
      "subscribersDir": "src/persistence/subscriber"
   }
}
